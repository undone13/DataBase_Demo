<Window x:Class="DataBase_Demo.adresa"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataBase_Demo"
        mc:Ignorable="d"
        Title="adresa" Height="210" Width="500">
    <StackPanel Margin="60, 0">
        <Label Content="Address"/>
        <TextBox x:Name="textbox_address"/>
        <Label Content="Phone Number"/>
        <TextBox x:Name="textbox_pnumber"/>
        <Label Content="Email Address"/>
        <TextBox x:Name="textbox_email"/>
        <Button x:Name="button_submit" Content="Submit" Margin="50,5" Click="button_submit_Click"/>
    </StackPanel>
</Window>
