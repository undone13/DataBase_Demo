<Window x:Class="DataBase_Demo.afisare_date"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DataBase_Demo"
        mc:Ignorable="d"
        Title="User Data" Height="150" Width="450">
    <StackPanel>
        <Label
            x:Name="label_username"
            Content="Username : "
            />
        <Label
            x:Name="label_address"
            Content="Address : "
            />
        <Label
            x:Name="label_phonenumber"
            Content="Phone Number : "
            />
        <Label
            x:Name="label_email"
            Content="Email : "
            />
        <Label
            x:Name="label_userid"
            Content="User Id : "
            />
    </StackPanel>
</Window>
